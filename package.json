{
    "name": "react-fullpagejs-example",
    "version": "1.0.0",
    "description": "https://github.com/alvarotrigo/react-fullpage",
    "keywords": [
        "react-fullpage",
        "component",
        "fullpage.js"
    ],
    "main": "src/index.js",
    "dependencies": {
        "@fullpage/react-fullpage": "^0.1.0",
        "react": "16.3.2",
        "react-dom": "16.3.2",
        "react-scripts": "1.1.4"
    },
    "devDependencies": {
        "node-sass": "^5.0.0",
        "npm-run-all": "^4.1.5"
    },
    "scripts": {
        "build-css": "node-sass --include-path ./src --include-path   ./node_modules src/ -o src/",
        "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
        "start": "npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-scripts build",
        "start-js": "react-scripts start",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    }
}
